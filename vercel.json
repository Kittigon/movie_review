{
  "version": 2,
  "builds": [
    {
      "src": "movie-search/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "movie-search/dist"
      }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
