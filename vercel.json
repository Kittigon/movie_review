{
  "version": 2,
  "installCommand": "npm install && npm install --prefix movie-search",
  "buildCommand": "npm run build --prefix movie-search",
  "outputDirectory": "movie-search/dist",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
